<h1>Users</h1>

<table border="0">
  <thead>
  	<tr>
  		<th>image</th>
  		<th>name</th>
  		<th colspan="1"></th>
  	</tr>
  </thead>
  <% @users.each do |user| %>
  <tbody>
    <tr>
      <td><%= image_tag user.get_profile_image(100,100) %></td>
      <td><%= user.name %></td>
      <td><%= link_to "Show", user_path(user.id) %></td>
    </tr>
  </tbody>
  <% end %>
</table>